class Main
{
    public int fact(int n)
    {
        int f=1, i;
        for(i=1; i<=n;i++)
        {
            f=f*i;
        
        }
        return f;


    }
    public static void Main(String[]args);
    {
        int x=5;
        Main obj=new Main();
           int result=obj.fact(x);
    System.out.println("Factorial is: "+ result);
}
}
